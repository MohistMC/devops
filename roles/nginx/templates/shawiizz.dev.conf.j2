server {
    listen 80;
    listen [::]:80;

    server_name shawiizz.dev www.shawiizz.dev;

    location / {
        proxy_pass http://127.0.0.1:24000/;
    }

    add_header Access-Control-Allow-Origin "*";
}

server {
    listen 80;
    listen [::]:80;

    server_name 12.shawiizz.dev www.12.shawiizz.dev;

    root /var/www/douze;
    index index.html index.htm index.php;

    location / {
        try_files $uri $uri/ =404;
    }

    add_header Access-Control-Allow-Origin "*";
}